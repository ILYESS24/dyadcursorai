# Configuration pour Bolt AI Builder

# Clé API Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# URL de base de l'application
PUBLIC_APP_URL=http://localhost:5173

# Configuration de développement
NODE_ENV=development

# Configuration pour WebContainer
PUBLIC_WEBCONTAINER_ENABLED=true

# Configuration pour le déploiement
PUBLIC_DEPLOYMENT_URL=https://your-app.vercel.app

# Configuration des fonctionnalités
PUBLIC_AI_FEATURES_ENABLED=true
PUBLIC_EXPORT_FEATURES_ENABLED=true

# Configuration de sécurité
PUBLIC_CSP_ENABLED=true
